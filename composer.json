{
    "name": "turnaev/composer-patcher-plugin",
    "type": "composer-plugin",
    "license": "GPL-2.0",
    "description": "Composer plugin for source patching",
    "keywords": [
        "composer",
        "composer-plugin",
        "patches",
        "plugin",
        "files"
    ],
    "authors": [
        {
            "name": "Vladimir Turnaev",
            "email": "turnaev@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.3.2",
        "composer-plugin-api": "^1.0",
        "symfony/console": "*",
        "symfony/finder": "*",
        "symfony/filesystem": "*",
        "symfony/process": "*",
        "symfony/yaml": "*",
        "symfony/config": "*"
    },
    "require-dev": {
        "composer/composer": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Tvi\\Composer\\": "src/"
        }
    },
    "extra": {
        "class": "Tvi\\Composer\\Plugin",
        "branch-alias": {
            "dev-master": "dev-master"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
